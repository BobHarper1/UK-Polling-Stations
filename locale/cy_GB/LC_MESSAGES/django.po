#: polling_stations/templates/home.html:21
msgid "Find your Polling Station"
msgstr "Dod o hyd i'ch gorsaf bleidleisio"

#: polling_stations/templates/postcode_view.html:17
msgid "Your polling station"
msgstr "Eich gorsaf bleidleisio"

#: polling_stations/templates/postcode_view.html:20
#, fuzzy, python-format
#| msgid "Phone %(council.name)s"
msgid "Contact %(council_name)s"
msgstr "Ffoniwch Gyngor %(council.name)s"

#: polling_stations/templates/postcode_view.html:37
#, python-format
msgid "About %(walk_time)s walk"
msgstr "Tua %(walk_time)s o gerdded"

#: polling_stations/templates/postcode_view.html:46
#, python-format
msgid ""
"\n"
"          <p>We don't have data for your area, so you need to contact the "
"council.</p>\n"
"          <p>You can call them on <strong><a href=\"tel:%(council_phone)s\">"
"%(council_phone)s</a></strong></p>\n"
"          "
msgstr ""

#: polling_stations/templates/postcode_view.html:64
msgid ""
"\n"
"        <h3>You don't need your poll card to vote</h3>\n"
"        <p>If you don't have your poll card, you can go to the polling\n"
"            station and give them your name and address.</p>\n"
"        <p>You don't need any other form of ID.</p>\n"
"        <p>If you haven’t received a polling card but think you should have "
"done, contact your council</p>\n"
"        <p><strong>Polling stations are open from 7am to 10pm on polling day."
"</strong></p>\n"
"        <p><a href=\"https://www.gov.uk/voting-in-the-uk/polling-stations\" "
"target=\"_blank\">\n"
"          Read more about voting in the UK\n"
"        </a></p>\n"
"        "
msgstr ""

